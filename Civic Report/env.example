VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Blockchain (optional)
# Deployed ComplaintRegistry contract address (e.g., 0xabc...)
VITE_COMPLAINT_REGISTRY_ADDRESS=
# NFT.Storage token to pin complaint metadata JSON
VITE_NFT_STORAGE_TOKEN=

# AI (optional)
# Google Generative AI (Gemini) API key for auto-fill from media
VITE_GEMINI_API_KEY=

# AI Provider selection:
#   gemini   -> Use Google Generative AI (default)
#   openrouter -> Use OpenRouter provider (vision models like gpt-4o-mini)
#   fallback -> Try Gemini first, fallback to OpenRouter on errors/quota
VITE_AI_PROVIDER=gemini

# OpenRouter (optional, used when VITE_AI_PROVIDER is 'openrouter' or 'fallback')
# Create a key at https://openrouter.ai/ and paste it below
VITE_OPENROUTER_API_KEY=
# Recommended lightweight vision model
VITE_OPENROUTER_MODEL=openai/gpt-4o-mini